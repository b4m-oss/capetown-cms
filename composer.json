{
    "name": "kohki-shikata/capetown-cms",
    "description": "CapetownCMS based on Laravel framework.",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Capetown\\": "capetown/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
